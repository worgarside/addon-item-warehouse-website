{
  "extends": [
    "next/core-web-vitals",
    "plugin:prettier/recommended",
    "plugin:@typescript-eslint/recommended-type-checked"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": true,
    "tsConfigRootDir": "."
  },
  "plugins": [
    "prettier",
    "@typescript-eslint"
  ],
  "root": true,
  "rules": {
    "prettier/prettier": "error"
  },
  "overrides":[{
  "files": ["**/*.js", "**/*.jsx", "**/*.ts", "**/*.tsx"]}]
}